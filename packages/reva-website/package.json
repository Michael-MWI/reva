{
  "name": "reva-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "graphql:generate": "graphql-codegen",
    "graphql:watch": "graphql-codegen --watch",
    "predev": "only-include-used-icons && npm run graphql:generate",
    "prebuild": "only-include-used-icons && node ./src/scripts/generateRobotsTxt.js && npm i --include=dev --prefix ../reva-api && npm run graphql:generate --prefix ../reva-api && npm run graphql:generate",
    "cypress:open":"cypress open"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^0.66.6",
    "@headlessui/react": "^1.7.14",
    "@hookform/resolvers": "^2.9.11",
    "@next/font": "13.2.4",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "eslint": "8.34.0",
    "eslint-config-next": "13.2.4",
    "graphql-request": "^5.1.0",
    "lodash": "^4.17.21",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.1",
    "typescript": "4.9.5",
    "use-debounce": "^9.0.4",
    "zod": "^3.20.6"
  },
  "importSort": {
    ".js, .ts, .jsx, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@parcel/watcher": "^2.3.0",
    "@types/lodash": "^4.14.195",
    "autoprefixer": "^10.4.13",
    "cypress": "^13.3.0",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.2",
    "tailwindcss": "^3.2.6"
  }
}
