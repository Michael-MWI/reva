{
  "name": "reva-candidate",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "10",
    "node": "22"
  },
  "scripts": {
    "predev": "react-dsfr update-icons && graphql-codegen --config codegen.ts",
    "dev": "next dev -p 3004",
    "prebuild": "react-dsfr update-icons && npm i --include=dev --prefix ../reva-api && npm run graphql:generate --prefix ../reva-api && graphql-codegen --config codegen.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "graphql:generate": "graphql-codegen --config codegen.ts",
    "graphql:watch": "graphql-codegen --config codegen.ts --watch",
    "cypress:open": "cypress open",
    "cypress:ci": "cypress run",
    "test:ci": "start-server-and-test 'next start -p 3004' http://localhost:3004/candidat/ cypress:ci"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.23.4",
    "@hookform/resolvers": "^4.1.3",
    "@tanstack/react-query": "^5.76.1",
    "@urql/exchange-auth": "^2.2.1",
    "classnames": "^2.5.1",
    "cookies-next": "^5.1.0",
    "date-fns": "^4.1.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.1.2",
    "html2pdf.js": "^0.10.3",
    "keycloak-js": "^24.0.3",
    "next": "15.3.2",
    "postcss-url": "^10.1.3",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.56.3",
    "react-hot-toast": "^2.5.2",
    "sharp": "^0.34.1",
    "urql": "^4.2.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.4",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/client-preset": "4.8.1",
    "@parcel/watcher": "^2.5.1",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10",
    "cypress": "^14.3.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "sass": "^1.89.0",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3"
  }
}
