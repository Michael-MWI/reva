{
  "name": "reva-interop",
  "version": "0.0.1",
  "type": "module",
  "description": "France VAE's interoperability API to interface with other official entities",
  "engines": {
    "npm": "10",
    "node": "22"
  },
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon -e ts,json,graphql --exec tsx -w . ./index.ts",
    "prebuild": "npm i --include=dev --prefix ../reva-api && npm run graphql:generate --prefix ../reva-api && graphql-codegen --config codegen.ts",
    "graphql:generate": "graphql-codegen --config codegen.ts",
    "graphql:watch": "graphql-codegen --config codegen.ts --watch",
    "build": "tsc",
    "start": "npm run prisma:migrate:deploy && node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:generate:migration": "prisma migrate dev --create-only",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --color routes utils",
    "postinstall": "npm i --include=dev --prefix ../reva-api && npm run graphql:generate --prefix ../reva-api && graphql-codegen --config codegen.ts"
  },
  "author": "",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@parcel/watcher": "^2.5.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "json-schema-to-ts": "^3.1.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "prisma": "^6.13.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/multipart": "^9.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-json-schema-to-ts": "^5.0.0",
    "@prisma/client": "^6.13.0",
    "@urql/core": "^5.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.2",
    "eslint-plugin-prettier": "^5.4.1",
    "fastify": "^5.4.0",
    "jose": "^6.0.11",
    "pino-datadog-transport": "^3.0.6"
  }
}
