{
  "name": "reva-admin-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "only-include-used-icons && graphql-codegen --config codegen.ts",
    "dev": "next dev -p 3003",
    "prebuild": "only-include-used-icons && npm i --include=dev --prefix ../reva-api && npm run graphql:generate --prefix ../reva-api && graphql-codegen --config codegen.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "graphql:generate": "graphql-codegen --config codegen.ts",
    "graphql:watch": "graphql-codegen --config codegen.ts --watch"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.0.4",
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.4.3",
    "@types/crypto-js": "^4.2.1",
    "crisp-sdk-web": "^1.0.21",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.0.6",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "jwt-decode": "^4.0.0",
    "keycloak-js": "^19.0.3",
    "next": "14.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@parcel/watcher": "^2.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "postcss": "^8",
    "postcss-nesting": "^12.0.2",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "tailwindcss": "^3",
    "typescript": "^5.2.2"
  }
}
